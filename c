#include <stdio.h>

int main(){

   int array[6][8]={1, 2, 3, 4, 5, 6, 7, 8, 2, 3, 4, 5, 6, 7, 8, 9, 3, 4, 5, 6, 7, 8, 9, 10, 4, 5, 6, 7, 8, 9, 10, 11, 5, 6, 7, 8, 9, 10, 11, 12, 6, 7, 8, 9, 10, 11, 12, 13};
   int x[8]={1, -8, 3, -6, 5, -4, 7, -2};
   int y[6]={0};

   for(int i=0;i<6;i++){
       printf("\n");
       for(int j=0; j<8; j++){
           printf("%7d",array[i][j]);
       }

}
    printf("\n---------------------------------------------------------\n");
   for(int i=0;i<8;i++)
       printf("%d\t",x[i]);

   for(int i=0;i<6;i++){
       for(int j=0;j<8;j++){
           y[i]+=array[i][j]*x[j];
       }
   }
    printf("\n---------------------------------------------------------\n");
    for(int i=0;i<6;i++)
        printf("%d\t",y[i]);

    return 0;
}
