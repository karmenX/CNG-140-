#include <stdio.h>

int main( ){
int array[10]={45,7,9,4,90,56,100,2,5,10};
int input,check=0;
printf("Enter a number to search:");
scanf("%d",&input);
for(int i=0; i<10; i++) {
     if(array[i]==input){
         check=1;
     break;}
     }
if(check==1)
     printf("Number %d is in the array!",input); else
     printf("Number %d is not in the array!",input);

     return 0;
}
