#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 5
int main(){

    srand(time(NULL));
    int array[SIZE];
    int swapped;
    int temp;

    for(int i=0;i<SIZE;i++){
        array[i]=rand()%10;
        printf("array[%d]=%d\n",i,array[i]);
    }
    do{
        swapped=0;
        for(int i=1;i<SIZE;i++){
            if(array[i-1]>array[i]){
                temp=array[i-1];
                array[i-1]=array[i];
                array[i]=temp;
                swapped=1;
            }

        }
        printf("After pass: ");
        for(int i = 0; i < SIZE; i++) {
            printf("%d ", array[i]);
        }
        printf("\n");
    }while(swapped);


    return 0;
}
