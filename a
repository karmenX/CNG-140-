#include <stdio.h>

int main(){

    float rates[]={12.9, 19.6, 11.4, 13.7, 9.5, 15.2, 17.6};
    printf("Values stored in the array are as follows: \n");
    int size=sizeof(rates)/sizeof(rates[0]);
    printf("[");
    for(int i=0;i<size;i++){
    printf("%.1f",*(rates+i));
    if(i<size-1)
        printf(",");}

    printf("]");

    return 0;
}
