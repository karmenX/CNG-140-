#include <stdio.h>
int IsSumDigitsEven(int number ){
    int remainder=0,sum=0;
    while(number>0){
        remainder=number%10;
        sum=sum+remainder;
        number/=10;
    }
    if(sum%2==0)
        return 1;
    else
        return 0;
}

int main() {
    int value;
    printf("Enter a number:");
    scanf("%d",&value);
    if(IsSumDigitsEven(value)==1)
        printf("Sum of digits of number %d is even",value);
    else
        printf("Sum of digits of number %d is not even",value);
    return 0;
}
