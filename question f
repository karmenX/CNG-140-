#include <stdio.h>

int main() {
  int num,rem=0,result=0,place=1;
    printf("Enter  a decimal number: ");
    scanf("%d",&num);

    while(num){
        rem= num%2;
        num/=2;
        result= result+(rem*place);
        place=place*10;
    }
    printf("the result is %d",result);

    return 0;
}
